    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "webpack": "5.98.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "node": true,
