  "author": "aSH <ash.uncover@gmail.com>",
  "description": "Util project for all react games",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/ap-games-common"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
